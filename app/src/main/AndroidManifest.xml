<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sarnobatvipul.mydocapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mydocapplogo_box_final"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mydocapplogo_box_final"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".SignIn_Pt_Activity" />
        <activity android:name=".SignIn_Doc_Activity" />
        <activity android:name=".ptHomePage_Activity" />
        <activity android:name=".ptProfileActivity" />
        <activity android:name=".docHomePage_Activity" />
        <activity android:name=".docProfileActivity" />
        <activity android:name=".SignUp_pt_Activity" />
        <activity android:name=".SignUp_doc_Activity" />
        <activity android:name=".DisplayProfileActivity" />
        <activity android:name=".DisplayAProfileActivity" />
        <activity android:name=".ReminderActivity"></activity>
    </application>

</manifest>